<div class="col-sm-3">
    <div class="list-group">
        <a href="/dashboard" class="list-group-item list-group-item-action active">Dashboard</a>
        <a href="/profile" class="list-group-item list-group-item-action">Profile</a>
        <a href="/logout" class="list-group-item list-group-item-action">Logout</a>
        <a href="#" class="list-group-item list-group-item-action disabled" aria-disabled="true"><%= user.name %></a>
    </div>
    
    <div class="list-group mt-3">
        <li class="list-group-item list-group-item-secondary">Notes</li>
        <% if (typeof notesFive != 'undefined') { %>
         <% notesFive.forEach(item => { %>
          <a href="#" class="list-group-item list-group-item-action"><%= item.title %></a>
         <% }) %>
        <% } %>
    </div>

</div>